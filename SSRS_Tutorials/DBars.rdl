    <DataSet Name="SalesOrderHeader">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT 
CONVERT(VARCHAR(2),MONTH([OrderDate])) +'-'+ CONVERT(VARCHAR(4),YEAR([OrderDate])) AS 'MonthYear',
SUM([SubTotal]) AS Total
FROM [Sales].[SalesOrderHeader]
GROUP BY CONVERT(VARCHAR(2),MONTH([OrderDate])) +'-'+ CONVERT(VARCHAR(4),YEAR([OrderDate]))</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>

  <DataSets>
    <DataSet Name="SalesPersonDetails">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT 
P.FirstName+', '+P.LastName AS 'SalesPerson',
SalesQuota,
SP.SalesYTD,
SP.SalesLastYear
FROM [Sales].[SalesPerson] SP INNER JOIN [Person].[Person] P ON SP.BusinessEntityID=P.BusinessEntityID
WHERE SP.SalesQuota IS NOT NULL</CommandText>
      </Query>
